    "preversion": "npm run lint",
    "version": "git add -A .",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@vinorcola/lint": "^1.0.5",
    "typescript": "^5.9.3"
